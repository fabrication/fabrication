<?xml version="1.0" encoding="utf-8"?>
<fab:LayoutModule
	xmlns:fab="org.puremvc.as3.multicore.utilities.fabrication.addons.layouts.*"
    xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:s="library://ns.adobe.com/flex/spark">
	<fx:Script>
		<![CDATA[


        public function get routes():Array
        {
            return [
                {id:"harry", path:"harry", expr:self.harry, component:harry},
                {id:"ron", path:"ron", expr:self.ron, component:ron},
                {id:"hermione", path:"hermione", expr:self.hermione, component:hermione}
            ];
        }
			
		]]>
	</fx:Script>
	<s:Button id="harry" label="harry" />
	<s:Button id="ron" label="ron" />
	<s:Button id="hermione" label="hermione" />
</fab:LayoutModule>
