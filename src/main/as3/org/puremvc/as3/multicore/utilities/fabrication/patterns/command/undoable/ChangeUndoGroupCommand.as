	/**	 * Changes the current undo group in the fabrication controller. The body of the notification	 * is the name of the undo group to switch to.	 * 	 * @author Darshan Sawardekar	 */	public class ChangeUndoGroupCommand extends SimpleFabricationCommand {		/*		 * Changes the current undo group of the fabrication undo history.		 */		override public function execute(note:INotification):void {			fabFacade.fabricationController.groupID = note.getBody() as String;		} 			}}